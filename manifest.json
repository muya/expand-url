{
	"manifest_version": 2,
	"name": "__MSG_extName__",
	"description": "__MSG_extDesc__",
	"version": "0.1",
	"default_locale": "en",
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"author": "Fred Muya",
	"background": {
		"persistent": false,
		"scripts": [
			"js/jquery.min.js",
			"js/app-config.js",
			"js/background.js",
			"js/options.js"
		]
	},
	"options_page": "options.html",
	"permissions": [
		"contextMenus",
		"tabs",
		"storage",
		"http://*.unshorten.it/*",
		"https://*.cloudflare.com/*"
	]
}
